$(document).ready(function(){
	
	
	$( '.catalogue-card-price-cart .btn' ).click(
		function ()
		{
			var id = $( this ).data( 'id' );
			var writing = $( '[name="article"]' ).prop( 'checked' );
			
			if ( id )
			{
				var formData = new FormData();
				formData.append( 'id', id );
				formData.append( 'writing', writing );
				
				windowOpen( '/local/templates/e_pr_public_V2/windows/cart.php', formData );
			}
		}
	);
	
	
	$( '.order-cart-item-article input' ).change(
		function ()
		{
			var id = $( this ).closest( '.order-cart-item' ).data( 'id' );
			var writing = $( this ).prop( 'checked' );
			
			$.post(
				'/local/templates/e_pr_public_V2/ajax/change_cart.php',
				{
					id: id,
					writing: writing
				}
			);
		}
	);
	
	
	/*
	  tools.js
	
	$( 'form.ownd-order-form' ).submit(
		function ()
		{	
			$.post(
				'/local/templates/e_pr_public_V2/ajax/order.php',
				{
					comment: $( '[name="comment"]' ).val(),
					name: $( '[name="firstname"]' ).val(),
					lastname: $( '[name="lastname"]' ).val(),
					email: $( '[name="email"]' ).val(),
					password: $( '[name="password"]' ).val(),
				},
				function ( response )
				{
					if ( response.error )
					{
						$( '[name="password"]' ).addClass( 'error' ).removeClass( 'valid' );
						$( '[name="password"] + .form-input-notice' ).css( 'color', '#df8e8e' );
					}
					else
					{
						window.location.href = '/order/success/' + response.id + '/';
					}
				},
				'json'
			);
			
			return false;
		}
	);
	*/
	
	
	$( 'form.ownd-order-message-form' ).submit(
		function ()
		{
			var message = $( '.order-detail-new-message [name="message"]' ).val();
			$( '.order-detail-new-message [name="message"]' ).val( '' );
			
			$.post(
				'/local/templates/e_pr_public_V2/ajax/order_message.php',
				{
					orderId: $( '[name="orderId"]' ).val(),
					message: message,
				},
				function (mess)
				{
					$( '.order-detail-messages' ).append( '<div class="order-detail-message"><div class="order-detail-message-text"><div class="order-detail-message-text-inner">' + mess + '</div></div></div>' );
				}
			);
			
			return false;
		}
	);
	
	$( 'body' ).on(
		'click',
		'.window-attach .btn',
		function ()
		{
			var files = [];
			$( '.form-files-list-item' ).each(
				function ( index, element )
				{
					files.push( $( element ).data( 'url' ) );
				}
			);

			if ( files.length )
			{
				if ( $( '[name="orderId"]' ).length )
				{
					$.post(
						'/local/templates/e_pr_public_V2/ajax/order_message_file.php',
						{
							orderId: $( '[name="orderId"]' ).val(),
							files: files
						},
						function (mess)
						{
							$( '.order-detail-messages' ).append( '<div class="order-detail-message"><div class="order-detail-message-text"><div class="order-detail-message-text-inner">' + mess + '</div></div></div>' );
						}
					);
					
					$( '.window-attach-container' ).remove();
				}
				else if ( $( '.support-ticket-new-form' ).length )
				{
					$( '[name="files"]' ).val( JSON.stringify( files ) );
					$( '.support-ticket-new-form .btn' ).click();
				}
				else
				{
					$.post(
						'',
						{
							files: files
						},
						function ()
						{
							window.location.reload();
						}
					);
				}
			}
			
			return false;
		}
	);
	
	
	$( 'form.ownd-order-status-change' ).submit(
		function ()
		{
			var status = $( '.order-detail-status-select-item [name="status"]:checked' ).val();
			var comment = $( '[name="status-comment"]' ).val();
			
			$.post(
				'/local/templates/e_pr_public_V2/ajax/order_status.php',
				{
					orderId: $( '[name="orderId"]' ).val(),
					status: status,
					comment: comment,
				},
				function ()
				{
					window.location.reload();
				}
			);

			return false;
		}
	);
	
	$( '.order-detail-header-cancel a' ).click(
		function ()
		{
			$('.ownd-order-cancel-confirm').addClass('visible');
			return false;
		}
	);
	
	$('body').on('click', '.ownd-order-cancel-confirm .order-cart-confirm-apply', function(e) {
		$('.ownd-order-cancel-confirm').removeClass('visible');
		
		var orderId = $( '.order-detail-header-cancel a' ).data( 'id' );
		$( '.order-detail-header-cancel a' ).remove();
		
		$.post(
			'/local/templates/e_pr_public_V2/ajax/order_status.php',
			{
				orderId: orderId,
				status: 'Rejected-1552',
			},
			function ()
			{
				window.location.reload();
			}
		);
		
		e.preventDefault();
	});
	
	$('body').on('click', '.ownd-order-cancel-confirm .order-cart-confirm-cancel', function(e) {
		$('.ownd-order-cancel-confirm').removeClass('visible');
		e.preventDefault();
	});
	
	
	$( '.personal-profile-ctrl-edit' ).click(
		function ()
		{
			$( this ).hide();
			
			$( '.personal-profile-ctrl-cancel, .personal-profile-ctrl-save, #photo-editor' ).show();
			$( '.personal-profile-avatar-new' ).css( 'display', 'flex' );
			$( '.personal-profile-avatar' ).addClass( 'personal-profile-avatar-editable' );
			$( '.personal-profile-content-fields input' ).prop( 'disabled', false );
		}
	);
	
	$( '.personal-profile-ctrl-cancel' ).click(
		function ()
		{
			window.location.reload();
		}
	);
	
	
	$( '.ownd-order-messages-tab' ).click(
		function ()
		{
			var orderId = $( this ).data( 'id' );
			
			$.post(
				'/local/templates/e_pr_public_V2/ajax/read_order_messages.php',
				{
					orderId: orderId
				}
			);
		}
	);
	
	
	
});



function changeOrderDetails()
{
	var data = [];
	$( '.order-media-window-list .order-media-window-item' ).each(
		function ( index, element )
		{
			var item = {};
			item.name = $( element ).find( '.order-media-window-item-title' ).html();
			item.publicationCost = $( element ).find( '[name="publicationCost"]' ).val();
			item.writingCost = $( element ).find( '[name="writingCost"]' ).val();
			item.withWriting = $( element ).find( '[name="withWriting"]' ).prop( 'checked' );
			
			data.push( item );
		}
	);
	
	$.post(
		'/local/templates/e_pr_public_V2/ajax/change_order_details.php',
		{
			orderId: $( '.order-media-window-content form' ).data( 'id' ),
			data: data
		},
		function ()
		{
			window.location.reload();
		}
	);
	
	
	$( '.order-media-window' ).addClass( 'loading' );
    $( '.order-media-window-ctrl-save .btn' ).prop( 'disabled', true );
}
